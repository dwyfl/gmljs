{
  "name": "gmljs",
  "version": "2.0.5",
  "description": "Library for parsing GML documents.",
  "main": "lib/index.js",
  "scripts": {
    "clean": "rm -rf lib && rm -rf node_modules",
    "test": "npx jest test",
    "lint": "npx eslint .",
    "build": "npx babel src --out-dir lib",
    "develop": "npx babel --watch src --out-dir lib --source-maps inline",
    "prepublishOnly:": "rm -rf lib && npm run build"
  },
  "author": "Gustav Peterz <gpeterz@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/dwyfl/gmljs.git"
  },
  "babel": {
    "presets": [
      "@babel/preset-env"
    ],
    "plugins": [
      [
        "babel-plugin-inline-import",
        {
          "extensions": [
            ".xml",
            ".gml"
          ]
        }
      ]
    ]
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.2.2",
    "@babel/preset-env": "^7.3.1",
    "babel-bridge": "^1.12.11",
    "babel-jest": "^24.0.0",
    "babel-plugin-inline-import": "^3.0.0",
    "eslint": "^5.12.1",
    "jest": "^24.0.0"
  },
  "dependencies": {
    "xmldom": "^0.1.27"
  }
}
